	<ThingDef ParentName="BuildingBase">
		<defName>MM-SS-AA5HeavySpeeder_prop</defName>
		<label>A-A5 Heavy Speeder Prop</label>
		<description>A-A5 Heavy Speeder Prop</description>
		<rotatable>True</rotatable>
		<graphicData>
			<texPath>1_Repulsor/RepulsorTruck/truck</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(10,10)</drawSize>
			<drawOffset>(0,0,0)</drawOffset>
			<drawRotated>True</drawRotated>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<blockWind>true</blockWind>
		<castEdgeShadows>false</castEdgeShadows>
		<canOverlapZones>false</canOverlapZones>
		<statBases>
			<MaxHitPoints>1500</MaxHitPoints>
			<WorkToBuild>2000</WorkToBuild>
			<Flammability>0</Flammability>
		</statBases>
		<size>3,9</size>
		<building>
			<destroySound>BuildingDestroyed_Metal_Big</destroySound>
		</building>
		<costList>
			<Steel>150</Steel>
		</costList>
		<defaultPlacingRot>South</defaultPlacingRot>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<designationCategory>MM_StarWarsShipBuildTab</designationCategory>
		<designatorDropdown>MM-SS-AA5HeavySpeeder_Designator</designatorDropdown>
		<uiIconScale>0.75</uiIconScale>
